{
  "author": "Darin Haener <dphaener@gmail.com> (https://github.com/dphaener/reactable-forms)",
  "name": "reactable-forms",
  "description": "Form and schema building tools for React",
  "version": "0.1.17",
  "repository": {
    "type": "git",
    "url": "https://github.com/dphaener/reactable-forms"
  },
  "keywords": [
    "react",
    "forms",
    "form",
    "schema",
    "react-forms",
    "react-form",
    "react-schema",
    "property",
    "properties",
    "react-properties",
    "react-property"
  ],
  "license": "MIT",
  "main": "./lib/forms.js",
  "scripts": {
    "test": "jest",
    "example": "webpack-dev-server --config ./example/webpack.config.js --hot",
    "compile": "babel src --ignore __tests__ --stage 0 --out-dir lib; node-sass src/css/default.scss lib/css/default.css",
    "push": "npm run compile; npm publish ./"
  },
  "dependencies": {
    "pikaday": "^1.3.2",
    "underscore": "^1.8.3",
    "underscore-deep-extend": "0.0.5"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-jest": "^5.2.0",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.14.4",
    "file-loader": "^0.8.4",
    "img-loader": "^1.1.0",
    "jest-cli": "^0.4.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.1.2",
    "react-hot-loader": "^1.2.7",
    "react-tools": "^0.13.3",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=0.6"
  },
  "peerDependencies": {
    "react": ">=0.13"
  },
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "<rootDir>/__tests__/preprocessor.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "preprocessor.js"
    ]
  }
}
